	keywords = {Humans, Gene Regulatory Networks, Systems Biology, Cohort Studies, Female, Male, Genetic, Models, Statistical, Genetic Variation, Biological, Blood Glucose, Blood Glucose: chemistry, Diabetes Mellitus, Diabetes Mellitus: metabolism, Dyslipidemias, Dyslipidemias: genetics, Finland, Genetic Predisposition to Disease, Genetic Predisposition to Disease: genetics, {HDL}, {HDL}: chemistry, Lipoproteins, Lipoproteins: chemistry, Magnetic Resonance Spectroscopy, Magnetic Resonance Spectroscopy: methods, Phenotype}
