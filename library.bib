	keywords = {Reproducibility, Assay data, Bioconductor, Collaboration, Data science, Package, Rmarkdown, Rstats, Version control},
	file = {Full Text:/home/rcannood/Zotero/storage/ZCND5RTH/Finak et al. - 2018 - DataPackageR Reproducible data preprocessing, sta.pdf:application/pdf}
}

