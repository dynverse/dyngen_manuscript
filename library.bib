@article{pihur_reconstructiongeneticassociation_2008,
  title = {Reconstruction of Genetic Association Networks from Microarray Data: A Partial Least Squares Approach.},
  author = {Pihur, Vasyl and Datta, Somnath and Datta, Susmita},
