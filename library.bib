  author = {Pattyn, Filip and Hoebeeck, Jasmien and Robbrecht, Piet and Michels, Evi and De Paepe, Anne and Bottu, Guy and Coornaert, David and Herzog, Robert and Speleman, Frank and Vandesompele, Jo},
  file = {/home/rcannood/Zotero/storage/IT2T4YYI/Pattyn et al. - 2006 - methBLAST and methPrimerDB web-tools for PCR based methylation analysis.pdf},
