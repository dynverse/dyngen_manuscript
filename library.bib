  date = {2011-12},
  pages = {1940--8},
  keywords = {Humans,Neuroblastoma,Infant,Chromosome Aberrations,Neuroblastoma: genetics,Neuroblastoma: pathology,Nuclear Proteins,Nuclear Proteins: genetics,Oncogene Proteins,Oncogene Proteins: genetics,Prognosis,Prospective Studies,Recurrence,Survival Analysis},
