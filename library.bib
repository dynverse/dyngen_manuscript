  keywords = {Humans,Child,Neoplasms,Adolescent,Neoplasms: therapy,Treatment Outcome,Forecasting,Incidence,Neoplasms: epidemiology,Neoplasms: mortality,SEER Program,United States,United States: epidemiology},
  author = {a Smith, Malcolm and Seibel, Nita L and Altekruse, Sean F and Ries, Lynn a G and Melbert, Danielle L and O'Leary, Maura and Smith, Franklin O and Reaman, Gregory H},
  file = {/home/rcannood/Zotero/storage/75IVEU3N/Smith et al. - 2010 - Outcomes for children and adolescents with cancer challenges for the twenty-first century.pdf},
