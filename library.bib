  keywords = {Systems Biology,Gene Regulatory Networks,Humans,Genetic,Female,Male,Models,Biological,Phenotype,Cohort Studies,Genetic Predisposition to Disease,Genetic Predisposition to Disease: genetics,Statistical,Blood Glucose,Blood Glucose: chemistry,Diabetes Mellitus,Diabetes Mellitus: metabolism,Dyslipidemias,Dyslipidemias: genetics,Finland,Genetic Variation,HDL,HDL: chemistry,Lipoproteins,Lipoproteins: chemistry,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods},
