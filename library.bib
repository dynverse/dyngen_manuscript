	pmid = {23868073},
	keywords = {Statistics, Software, Proteins, Molecular Sequence Annotation, Internet, Databases, Molecular Sequence Annotation: methods, Nonparametric, Protein, Proteins: chemistry, Phylogeny, Proteins: classification},
	file = {Attachment:/home/rcannood/Zotero/storage/8378SWTE/Mi et al. - 2013 - Large-scale gene function analysis with the PANTHER classification system.pdf:application/pdf}
}

