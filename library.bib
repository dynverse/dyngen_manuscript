@article{papadopoulos_prossttprobabilisticsimulation_2018,
  title = {{{PROSSTT}}: {{Probabilistic Simulation}} of {{Single}}-{{Cell RNA}}-{{Seq Data}} for {{Complex Differentiation Processes}}},
