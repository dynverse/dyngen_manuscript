@article{aghaeepour_criticalassessmentautomated_2013,
	title = {Critical assessment of automated flow cytometry data analysis techniques.},
	volume = {10},
	issn = {1548-7105},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3906045&tool=pmcentrez&rendertype=abstract},
