  file = {/home/rcannood/Zotero/storage/F2I5TB9K/Kowalczyk et al. - 2015 - Single cell RNA-seq reveals changes in cell cycle and differentiation programs upon aging of hematopoietic ste.pdf},
  pmid = {26430063}
}

