  file = {/home/rcannood/Zotero/storage/3PUI6UGC/Pina et al. - 2015 - Single-Cell Network Analysis Identifies DDIT3 as a Nodal Lineage Regulator in Hematopoiesis.pdf;/home/rcannood/Zotero/storage/U8MKTPZK/Pina et al. - 2015 - Single-Cell Network Analysis Identifies DDIT3 as a Nodal Lineage Regulator in Hematopoiesis.pdf},
