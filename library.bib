  journaltitle = {Oncogene},
  date = {2001-11},
  pages = {7573--7},
  keywords = {Humans,DNA Methylation,Genetic,Neuroblastoma,Neuroblastoma: genetics,Prognosis,Human,Mutation,Phenotype,Genes,Molecular Sequence Data,Promoter Regions,Tumor Suppressor Proteins,DNA Mutational Analysis,Base Sequence,Adrenal Gland Neoplasms,Neoplasm Proteins,Neoplasm Proteins: genetics,Chromosomes,Alleles,Adrenal Gland Neoplasms: genetics,Antimetabolites,Antineoplastic,Antineoplastic: pharmacology,Azacitidine,Azacitidine: pharmacology,Caspase 8,Caspase 9,Caspases,Caspases: genetics,CpG Islands,CpG Islands: genetics,Gene Deletion,Loss of Heterozygosity,Microsatellite Repeats,Pair 3,Pheochromocytoma,Pheochromocytoma: genetics,Tumor Suppressor},
