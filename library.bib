@article{guo_slicedeterminingcell_2017,
  title = {\{\{\vphantom{\}\}}{{SLICE}}\vphantom\{\}\vphantom\{\}: {{Determining Cell Differentiation}} and {{Lineage Based}} on {{Single Cell Entropy}}},
