	number = {5},
	journaltitle = {Cell Metabolism},
	author = {Zeng, Chun and Mulas, Francesca and Sui, Yinghui and Guan, Tiffany and Miller, Nathanael and Tan, Yuliang and Liu, Fenfen and Jin, Wen and Carrano, Andrea C and Huising, Mark O and Shirihai, Orian S and Yeo, Gene W and Sander, Maike},
