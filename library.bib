	keywords = {Algorithms, Animals, Humans, Proteins, Systems Biology, Models, Biological, Systems Biology: methods, Proteins: metabolism},
	file = {Attachment:/home/rcannood/Zotero/storage/PZFGDBBV/Fionda, Palopoli - 2011 - Biological network querying techniques analysis and comparison.pdf:application/pdf}
}

