	pmcid = {PMC3687085},
	keywords = {Genome, Algorithms, Animals, Computational Biology, {DNA}-Binding Proteins, Mice, Nucleotide Motifs, Position-Specific Scoring Matrices, Protein Array Analysis, Transcription Factors},
