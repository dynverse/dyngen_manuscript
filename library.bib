  keywords = {Manifold learning,Data mining,Gene regulatory networks,Imputation,Pseudotime trends,Single cell RNA sequencing},
  author = {Moon, Kevin R and Stanley, Jay S and Burkhardt, Daniel and van Dijk, David and Wolf, Guy and Krishnaswamy, Smita},
  options = {useprefix=true}
