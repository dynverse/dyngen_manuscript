	file = {Attachment:/home/rcannood/Zotero/storage/LYLJEHFT/Bovolenta, Acencio, Lemke - 2012 - HTRIdb an open-access database for experimentally verified human transcriptional regulation interacti.pdf:application/pdf}
}

