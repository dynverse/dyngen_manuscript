  title = {{{arrEYE}}: A Customized Platform for High-Resolution Copy Number Analysis of Coding and Noncoding Regions of Known and Candidate Retinal Dystrophy Genes and Retinal Noncoding {{RNAs}}},
  shorttitle = {{{arrEYE}}},
  author = {Van Cauwenbergh, Caroline and Van Schil, Kristof and Cannoodt, Robrecht and Bauwens, Miriam and Van Laethem, Thalia and De Jaegere, Sarah and Steyaert, Wouter and Sante, Tom and Menten, Björn and Leroy, Bart P. and Coppieters, Frauke and De Baere, Elfride},
