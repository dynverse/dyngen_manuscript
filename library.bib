@article{cope_benchmarkaffymetrixgenechip_2004,
  title = {A Benchmark for {{Affymetrix GeneChip}} Expression Measures},
  author = {Cope, Leslie M. and Irizarry, Rafael A. and Jaffee, Harris A. and Wu, Zhijin and Speed, Terence P.},
