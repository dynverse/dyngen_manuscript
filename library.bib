  keywords = {Algorithms,Animals,Humans,Computer Simulation,Models,Neural Networks (Computer),Community Networks,Nerve Net,Nerve Net: physiology,Social Behavior,Theoretical},
  author = {Girvan, M and Newman, M E J},
