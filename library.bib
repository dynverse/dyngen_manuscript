  year = {2007},
  keywords = {Humans,Gene Expression Profiling,Neuroblastoma,Neuroblastoma: therapy,Neuroblastoma: genetics,Prognosis,Gene Expression Profiling: methods,Neuroblastoma: physiopathology},
  pages = {2106--2120},
