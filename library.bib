  keywords = {Software,Algorithms,Animals,Humans,Computational Biology,Computational Biology: methods,Databases,DNA Methylation,DNA Primers,DNA Primers: chemistry,Epigenesis,Genetic,Internet,Mice,Polymerase Chain Reaction,Polymerase Chain Reaction: methods,Protein,Rats,Sulfites,Sulfites: chemistry},
  pages = {496},
  file = {/home/rcannood/Zotero/storage/IT2T4YYI/Pattyn et al. - 2006 - methBLAST and methPrimerDB web-tools for PCR based methylation analysis.pdf},
