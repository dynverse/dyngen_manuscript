  keywords = {Humans,Child,Neoplasms,Adolescent,Neoplasms: therapy,Treatment Outcome,Forecasting,Incidence,Neoplasms: epidemiology,Neoplasms: mortality,SEER Program,United States,United States: epidemiology},
  pages = {2625--34},
