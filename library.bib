	author = {Margolin, Adam a and Nemenman, Ilya and Basso, Katia and Wiggins, Chris and Stolovitzky, Gustavo and Favera, Riccardo Dalla and Califano, Andrea and Dalla Favera, Riccardo and Califano, Andrea and Favera, Riccardo Dalla},
