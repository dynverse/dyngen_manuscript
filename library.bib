@article{stahl_visualizationanalysisgene_2016,
  title = {Visualization and Analysis of Gene Expression in Tissue Sections by Spatial Transcriptomics},
  author = {Ståhl, Patrik L. and Salmén, Fredrik and Vickovic, Sanja and Lundmark, Anna and Navarro, José Fernández and Magnusson, Jens and Giacomello, Stefania and Asp, Michaela and Westholm, Jakub O. and Huss, Mikael and Mollbrink, Annelie and Linnarsson, Sten and Codeluppi, Simone and Borg, Åke and Pontén, Fredrik and Costea, Paul Igor and Sahlén, Pelin and Mulder, Jan and Bergmann, Olaf and Lundeberg, Joakim and Frisén, Jonas},
