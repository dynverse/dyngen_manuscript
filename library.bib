	keywords = {Animals, Humans, Gene Expression Profiling, Gene Regulatory Networks, Mice, Transcriptome, Protein Interaction Maps, Intercellular Signaling Peptides and Proteins, Intercellular Signaling Peptides and Proteins: gen, {HeLa} Cells, Lysosomes, Lysosomes: ultrastructure},
