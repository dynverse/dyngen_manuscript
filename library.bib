  author = {Xu, Min and Kao, Ming-Chih J and Nunez-Iglesias, Juan and Nevins, Joseph R and West, Mike and Zhou, Xianghong Jasmine},
  file = {/home/rcannood/Zotero/storage/2NZAC8NZ/Xu et al. - 2008 - An integrative approach to characterize disease-specific pathways and their coordination a case study in cancer.pdf},
