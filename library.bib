  author = {Lin, Yingxin and Cao, Yue and Kim, Hani J and Salim, Agus and Speed, Terence P. and Lin, Dave and Yang, Pengyi and Yang, Jean Yee Hwa}
}

@article{lizio_gatewaysfantom5promoter_2015,
  title = {Gateways to the {{FANTOM5}} Promoter Level Mammalian Expression Atlas},
