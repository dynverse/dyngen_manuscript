  title = {The {{Branching Point}} in \{\vphantom\}{{Erythro}}-{{Myeloid}}\vphantom\{\} {{Differentiation}}},
  author = {Perié, Le\$\textbackslash backslash\$"\$\textbackslash backslash\$ila and Duffy, Ken R and Kok, Lianne and de Boer, Rob J and Schumacher, Ton N},
