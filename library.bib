  author = {Kuchaiev, Oleksii and Rasajski, Marija and Higham, Desmond J and Przulj, Natasa},
  file = {/home/rcannood/Zotero/storage/M9NYEKJE/Kuchaiev et al. - 2009 - Geometric de-noising of protein-protein interaction networks.pdf},
