@article{xu_constructionvalidationregulatory_2014,
  title = {Construction and {{Validation}} of a {{Regulatory Network}} for {{Pluripotency}} and {{Self}}-{{Renewal}} of {{Mouse Embryonic Stem Cells}}.},
  author = {Xu, Huilei and Ang, Yen-Sin and Sevilla, Ana and Lemischka, Ihor R and Ma'ayan, Avi},
