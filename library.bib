  file = {/home/rcannood/Zotero/storage/HMSUNKQC/Kim et al. - 2015 - Single-cell mRNA sequencing identifies subclonal heterogeneity in anti-cancer drug responses of lung adenocarcinoma.pdf},
  pmid = {26084335}
}

@article{schlitzer_identificationcdc1cdc2committed_2015,
