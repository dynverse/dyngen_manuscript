@article{cordero_tracingcoregulatorynetwork_2017,
  title = {\{\{\vphantom{\}\}}{{TRACING CO}}\vphantom\{\}\vphantom\{\}-\{\{\vphantom{\}\}}{{REGULATORY NETWORK DYNAMICS IN NOISY}}\vphantom\{\}\vphantom\{\}, \{\{\vphantom{\}\}}{{SINGLE}}\vphantom\{\}\vphantom\{\}-\{\{\vphantom{\}\}}{{CELL TRANSCRIPTOME TRAJECTORIES}}\vphantom\{\}\vphantom\{\}},
