	file = {Attachment:/home/rcannood/Zotero/storage/75IUT4V4/Margolin et al. - 2006 - ARACNE An Algorithm for the Reconstruction of Gene Regulatory Networks in a Mammalian Cellular Context.pdf:application/pdf}
