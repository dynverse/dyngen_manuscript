@article{powers_visualizationtradeoffevaluation_2015,
  title = {Visualization of {{Tradeoff}} in {{Evaluation}}: From {{Precision}}-{{Recall}} \& {{PN}} to {{LIFT}}, {{ROC}} \& {{BIRD}}},
  shorttitle = {Visualization of {{Tradeoff}} in {{Evaluation}}},
  author = {Powers, David M. W.},
