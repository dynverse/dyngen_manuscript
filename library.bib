  keywords = {Algorithms,Cluster Analysis,Computer-Assisted,Fourier Analysis,Gene Expression Profiling,Gene Expression Profiling: methods,Multigene Family,Multigene Family: physiology,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae Proteins: metabolism,Sample Size,Signal Processing},
