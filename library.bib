  author = {Vandesompele, Jo and De Preter, Katleen and Pattyn, Filip and Poppe, Bruce and Van Roy, Nadine and De Paepe, Anne and Speleman, Frank},
  file = {/home/rcannood/Zotero/storage/RBLKJHAD/Vandesompele et al. - 2002 - Accurate normalization of real-time quantitative RT-PCR data by geometric averaging of multiple internal co.pdf},
