	keywords = {Statistics - Machine Learning, Computer Science - Machine Learning, Computer Science - Artificial Intelligence, Computer Science - Information Retrieval, Statistics - Methodology},
	file = {arXiv\:1505.00401 PDF:/home/rcannood/Zotero/storage/745KQY6C/Powers - 2015 - Visualization of Tradeoff in Evaluation from Prec.pdf:application/pdf;arXiv.org Snapshot:/home/rcannood/Zotero/storage/DP8X8JQ8/1505.html:text/html}
}

