	keywords = {Algorithms, Software, Animals, Humans, Computational Biology, Mice, Internet, Genetic, Databases, Rats, Computational Biology: methods, {DNA} Methylation, Epigenesis, Protein, {DNA} Primers, {DNA} Primers: chemistry, Polymerase Chain Reaction, Polymerase Chain Reaction: methods, Sulfites, Sulfites: chemistry},
	file = {Attachment:/home/rcannood/Zotero/storage/IT2T4YYI/Pattyn et al. - 2006 - methBLAST and methPrimerDB web-tools for PCR based methylation analysis.pdf:application/pdf}
