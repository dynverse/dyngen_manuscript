  date = {2009-05-08},
  pages = {387-397},
  keywords = {Animals,Cell Differentiation,Gene Regulatory Networks,Cell Lineage,Humans,Stem Cells,Hematopoiesis},
  author = {Enver, Tariq and Pera, Martin and Peterson, Carsten and Andrews, Peter W.},
